#include <iostream>
#include <cmath>
using namespace std;


void leadersInArray(int arr[], int n)
{
	int current_leader = arr[n - 1];

	cout<<current_leader<<" ";

	for(int i = n - 2; i >= 0; i--)
	{
		if(current_leader < arr[i])
		{
			current_leader = arr[i];

			cout<<current_leader<<" ";
		}
	}
}



int main() {
	
      int arr[] = {7, 10, 4, 10, 6, 5, 2}, n = 7;

      leadersInArray(arr, n);
    
}
